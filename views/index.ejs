  <main>
    <section>
      <h2>
        <%= squad.name %>
      </h2>
      <!-- Hier komen de members -->
      <% squad.members.forEach(member=> { %>
        <article>
          <% if (member.avatar) { %>
            <img src="<%= member.avatar %>" alt="De avatar van <%= member.name %>"
              onerror="this.onerror=null;this.src='/img/placeholder.jpeg';" />
            <% }else{ %>
              <img src="/img/placeholder.jpeg" alt="Placeholder avatar" />
              <% } %>
                <h2 class="name__card">
                  <%= member.name %>
                    <%= member.prefix %>
                      <%= member.surname %>
                        </h3>

                        <button class="card__button"><a href="<%= member.gitHubHandle %>"></a>
                          <h3 class="button__text">Github</h3>
                        </button>
        </article>
        <% }) %>
    </section>
  </main>








